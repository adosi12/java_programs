/*package whatever //do not write package name here */

import java.io.*;

void add(int, int);
void add(double,double);

class Adder{
	void add(int a, int b){
		System.out.println(“sum =”+(a+b));
	}

	void add(double a, double b){
		System.out.println(“sum=”+(a+b));
	}
	
	public static void main(String[] args){
		Adder ad=new Adder();
		ad.add(5,6);
		ad.add(5.4,7.2);
}}
